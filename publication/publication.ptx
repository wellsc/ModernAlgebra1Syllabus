<?xml version="1.0" encoding="UTF-8" ?>
<publication>
    <common>
        <!-- Chunking                                  -->
        <!-- 0 is an entire book in one file           -->
        <!-- 1 each top-level division is its own file -->
        <chunking level="1"/>
        <!-- Table of Contents                         -->
        <!-- 0 No table of contents                    -->
        <!-- 1 Top-level only shown in TOC             -->
        <!-- 2 Top two levels shown in TOC             -->
        <tableofcontents level='2'/>
    </common>
    <!-- directories are relative to the main source PreTeXt file -->
    <source>
        <directories external="../assets" generated="../generated-assets"/>
    </source>
<latex print="no">
  <page>
    <geometry>letterpaper,margin=0.75in,headheight=0.25in,includefoot</geometry>
  </page>
</latex>
</publication>
